{
  "connection": "default",
  "collectionName": "infos",
  "info": {
    "name": "info",
    "description": "Studio information such as Address, WiFi, Phone number, et."
  },
  "options": {
    "increments": true,
    "timestamps": [
      "created_at",
      "updated_at"
    ],
    "comment": ""
  },
  "attributes": {
    "address-line-1": {
      "type": "string"
    },
    "wifi-name": {
      "type": "string"
    },
    "wifi-password": {
      "type": "string"
    },
    "phone": {
      "type": "string"
    },
    "email": {
      "type": "email"
    },
    "address-line-2": {
      "type": "string"
    }
  }
}